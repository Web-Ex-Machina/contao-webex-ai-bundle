{
    "name": "webexmachina/webex-ai",
    "version": "0.0.1-dev",
    "minimum-stability" : "dev",
    "description": "Contao 5 WebEx AI Bundle.",
    "type": "contao-bundle",
    "license": "Apache-2.0",
    "website" : "https://www.webexmachina.fr/",
    "authors": [
        {
            "name": "Web ex Machina",
            "homepage": "https://www.webexmachina.fr",
            "role":"Developer"
        }
    ],
    "require": {
        "php": "^8.2",
        "contao/core-bundle": "5.*",
        "webexmachina/contao-utils": "^2.2",
        "symfony/cache": "6.4.x | 7.2.x"
    },
    "autoload":{
        "psr-4": {
            "WEM\\WebExAIBundle\\": "src/"
        }
    },
    "scripts": {
        "rector": "vendor/bin/rector p --ansi",
        "rector-dry": "vendor/bin/rector p --dryrun --ansi",
        "check-cs": "vendor/bin/ecs check --ansi",
        "fix-cs": "vendor/bin/ecs check --fix --ansi"
    },
    "extra":{
        "contao-manager-plugin": "WEM\\WebExAIBundle\\ContaoManager\\Plugin"
    },
    "config": {
        "allow-plugins": {
            "contao-components/installer": true,
            "php-http/discovery": true,
            "contao/manager-plugin": true,
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    },
    "conflict": {
        "contao/manager-plugin": "<2.0 || >=3.0"
    },
    "require-dev": {
        "contao/manager-plugin": "@stable",
        "contao/easy-coding-standard": "@stable",
        "rector/rector": "@stable",
        "phpstan/phpstan": "@stable",
        "contao/contao-rector": "dev-main"
    }
}
